{
    "libraryName": "amazonka-glacier",
    "operationPlugins": {
        "*": ["Request.glacierVersionHeader (Core._svcVersion defaultService)"]
    },
    "typeOverrides": {
        "InitiateMultipartUploadInput": {
            "requiredFields": [
                "partSize"
            ]
        },
        "UploadMultipartPartInput": {
            "requiredFields": [
                "range",
                "checksum"
            ]
        },
        "CompleteMultipartUploadInput": {
            "requiredFields": [
                "archiveSize",
                "checksum"
            ]
        }
    }
}
